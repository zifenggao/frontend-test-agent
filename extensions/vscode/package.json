{
  "name": "frontend-test-agent",
  "displayName": "Frontend Test Agent",
  "description": "AI-powered frontend testing automation right in your VS Code",
  "version": "1.0.2",
  "publisher": "zifenggao",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "test",
    "testing",
    "jest",
    "cypress",
    "playwright",
    "react",
    "vue",
    "angular",
    "AI",
    "automation"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:frontend-test-agent.generateTests",
    "onCommand:frontend-test-agent.runTests",
    "onCommand:frontend-test-agent.showCoverage",
    "onCommand:frontend-test-agent.analyzeResults",
    "onFileSystem:file"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "frontend-test-agent.generateTests",
        "title": "Generate Tests",
        "category": "Frontend Test Agent",
        "icon": "$(beaker)"
      },
      {
        "command": "frontend-test-agent.runTests",
        "title": "Run Tests",
        "category": "Frontend Test Agent",
        "icon": "$(play)"
      },
      {
        "command": "frontend-test-agent.showCoverage",
        "title": "Show Test Coverage",
        "category": "Frontend Test Agent",
        "icon": "$(pie-chart)"
      },
      {
        "command": "frontend-test-agent.analyzeResults",
        "title": "Analyze Test Results",
        "category": "Frontend Test Agent",
        "icon": "$(search)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "frontend-test-agent.generateTests",
          "when": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue",
          "group": "Frontend Test Agent@1"
        },
        {
          "command": "frontend-test-agent.runTests",
          "when": "resourceExtname == .test.js || resourceExtname == .test.ts || resourceExtname == .spec.js || resourceExtname == .spec.ts",
          "group": "Frontend Test Agent@2"
        }
      ],
      "editor/context": [
        {
          "command": "frontend-test-agent.generateTests",
          "when": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue",
          "group": "Frontend Test Agent@1"
        }
      ],
      "commandPalette": [
        {
          "command": "frontend-test-agent.generateTests",
          "when": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue"
        }
      ]
    },
    "configuration": {
      "title": "Frontend Test Agent",
      "properties": {
        "frontend-test-agent.modelProvider": {
          "type": "string",
          "default": "openai",
          "enum": [
            "openai",
            "volcengine",
            "anthropic",
            "google"
          ],
          "description": "AI model provider for test generation"
        },
        "frontend-test-agent.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API key for AI-powered test generation"
        },
        "frontend-test-agent.openaiModel": {
          "type": "string",
          "default": "gpt-3.5-turbo",
          "description": "OpenAI model to use for test generation"
        },
        "frontend-test-agent.volcengineApiKey": {
          "type": "string",
          "default": "",
          "description": "Volcengine API key for AI-powered test generation"
        },
        "frontend-test-agent.volcengineModel": {
          "type": "string",
          "default": "ep-20240722180546-xqxzh",
          "description": "Volcengine model to use for test generation"
        },
        "frontend-test-agent.volcengineBaseUrl": {
          "type": "string",
          "default": "https://ark.cn-beijing.volces.com/api/v3",
          "description": "Volcengine API base URL"
        },
        "frontend-test-agent.defaultFramework": {
          "type": "string",
          "default": "react",
          "enum": [
            "react",
            "vue",
            "angular"
          ],
          "description": "Default frontend framework"
        },
        "frontend-test-agent.defaultTestRunner": {
          "type": "string",
          "default": "jest",
          "enum": [
            "jest",
            "cypress",
            "playwright"
          ],
          "description": "Default test runner"
        },
        "frontend-test-agent.testOutputDirectory": {
          "type": "string",
          "default": "__tests__",
          "description": "Directory to generate test files into"
        },
        "frontend-test-agent.enableCoverage": {
          "type": "boolean",
          "default": true,
          "description": "Enable test coverage reporting"
        },
        "frontend-test-agent.enableAutoGenerate": {
          "type": "boolean",
          "default": false,
          "description": "Automatically generate tests when saving files"
        },
        "frontend-test-agent.autoGenerateDelay": {
          "type": "number",
          "default": 2000,
          "description": "Delay in milliseconds before auto-generating tests after save"
        }
      }
    },
    "keybindings": [
      {
        "command": "frontend-test-agent.generateTests",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g",
        "when": "editorTextFocus && (resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue)"
      },
      {
        "command": "frontend-test-agent.runTests",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus && (resourceExtname == .test.js || resourceExtname == .test.ts || resourceExtname == .spec.js || resourceExtname == .spec.ts)"
      }
    ],
    "views": {
      "test": [
        {
          "id": "frontend-test-agent.testExplorer",
          "name": "Test Explorer",
          "icon": "$(beaker)",
          "contextualTitle": "Frontend Test Agent"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "frontend-test-agent",
          "title": "Test Agent",
          "icon": "$(beaker)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "node-fetch": "^2.6.7",
    "openai": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/frontend-test-agent.git"
  },
  "homepage": "https://github.com/yourusername/frontend-test-agent/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/yourusername/frontend-test-agent/issues"
  }
}
