    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}