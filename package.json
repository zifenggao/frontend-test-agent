{
  "name": "frontend-test-agent",
  "version": "1.0.2",
  "description": "AI-powered frontend testing agent that automatically generates test cases, runs tests, and analyzes results",
  "main": "dist/index.js",
  "bin": {
    "test-agent": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli/index.js",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "frontend",
    "testing",
    "automation",
    "ai",
    "agent",
    "jest",
    "cypress",
    "playwright"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.24.7",
    "@babel/traverse": "^7.24.7",
    "@babel/types": "^7.24.7",
    "commander": "^12.1.0",
    "openai": "^4.52.3",
    "@volcengine/openapi": "^1.0.0",
    "node-fetch": "^3.3.2",
    "jest": "^29.7.0",
    "playwright": "^1.45.1",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "ora": "^8.0.1",
    "glob": "^10.4.1",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",

    "@types/babel__traverse": "^7.20.6",
    "@types/jest": "^29.5.12",
